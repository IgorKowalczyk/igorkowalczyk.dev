    "clsx": "2.1.1",
    "contentlayer2": "0.5.0",
    "dotenv": "16.4.5",
    "geist": "1.3.1",
    "github-slugger": "2.0.0",
    "million": "3.1.11",
    "next": "14.2.6",
    "next-contentlayer2": "0.5.0",
    "next-nprogress-bar": "2.3.13",
    "next-themes": "0.3.0",
    "nprogress": "0.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-parallax-tilt": "1.7.237",
    "react-wrap-balancer": "1.1.1",
