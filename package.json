{
  "name": "igorkowalczyk-portfolio",
  "description": "My portfolio built on Next.js",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "NODE_ENV=development next dev",
    "dev:turbo": "NODE_ENV=development next dev --turbo --show-all",
    "build": "next build",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier . --write --cache --ignore-unknown --plugin=prettier-plugin-tailwindcss",
    "format:check": "prettier . --check --cache --ignore-unknown",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@headlessui/tailwindcss": "0.2.0",
    "@heroicons/react": "2.1.1",
    "@vercel/analytics": "1.1.1",
    "clsx": "2.0.0",
    "contentlayer": "0.3.4",
    "dotenv": "16.3.1",
    "geist": "1.2.0",
    "github-slugger": "2.0.0",
    "next": "14.0.4",
    "next-contentlayer": "0.3.4",
    "next-nprogress-bar": "2.1.2",
    "next-themes": "0.2.1",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-parallax-tilt": "1.7.179",
    "react-wrap-balancer": "1.1.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-pretty-code": "0.12.3",
    "rehype-slug": "6.0.0",
    "remark-gfm": "3.0.1",
    "server-only": "0.0.1",
    "sharp": "0.33.1",
    "swr": "2.2.4",
    "validator": "13.11.0"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "2.2.0",
    "@igorkowalczyk/is-browser": "5.0.2",
    "@igorkowalczyk/prettier-config": "2.2.0",
    "@next/bundle-analyzer": "14.0.4",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.10",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "globby": "14.0.0",
    "mini-svg-data-uri": "1.4.4",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "tailwind-gradient-mask-image": "1.1.0",
    "tailwindcss": "3.4.0",
    "tailwindcss-text-fill": "0.2.0"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/igorkowalczyk.github.io.git"
  },
