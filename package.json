{
  "name": "igorkowalczyk-portfolio",
  "description": "My portfolio built on Next.js",
  "version": "4.3.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "NODE_ENV=development next dev",
    "dev:turbo": "NODE_ENV=development next dev --turbo --show-all",
    "build": "next build && pnpm run postbuild",
    "postbuild": "NODE_NO_WARNINGS=1 node ./lib/scripts/generate-rss.mjs",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier --write . --cache --ignore-unknown",
    "format:check": "prettier --check --cache --ignore-unknown .",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.14",
    "@headlessui/tailwindcss": "0.1.3",
    "@heroicons/react": "2.0.17",
    "@tippyjs/react": "4.2.6",
    "@vercel/analytics": "1.0.0",
    "@vercel/og": "0.5.3",
    "clsx": "1.2.1",
    "contentlayer": "0.3.1",
    "dotenv": "16.0.3",
    "feed": "4.2.2",
    "github-slugger": "2.0.0",
    "next": "13.3.1",
    "next-contentlayer": "0.3.1",
    "next-themes": "npm:@wits/next-themes@0.2.16",
    "nprogress": "0.2.0",
    "plaiceholder": "2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-parallax-tilt": "1.7.130",
