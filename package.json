  "version": "5.0.0",
  "author": "Igor Kowalczyk",
  "homepage": "https://igorkowalczyk.dev",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "NODE_ENV=development next dev",
    "dev:turbo": "NODE_ENV=development next dev --turbo --show-all",
