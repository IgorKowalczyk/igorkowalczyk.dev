    "rehype-code-titles": "1.2.0",
    "rehype-prism-plus": "1.5.1",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1",
    "server-only": "0.0.1",
    "sharp": "0.32.0",
    "swr": "2.1.3",
    "validator": "13.9.0"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "1.2.0",
    "@igorkowalczyk/is-browser": "3.1.0",
    "@igorkowalczyk/prettier-config": "1.2.0",
    "@next/bundle-analyzer": "13.3.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
