    "postcss": "8.4.23",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "0.2.7",
    "tailwind-gradient-mask-image": "1.0.0",
    "tailwindcss": "3.3.1",
    "tailwindcss-text-fill": "0.2.0",
    "typescript": "5.0.4"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
